<div id="langmenu" class="langmenu">
{% for aTarget in aTargetList %}
    <a href="{{ aTarget.sTargetPageUrl }}" {% if aTarget.bCurrentLanguage %}class="current" {% endif %}title="{{ aTarget.sToolTip }}">
        <span>
            <img src="{{ aTarget.sIconUrl }}flags/{{ aTarget.sImageType }}/{{ aTarget.sFilename|lower }}-min.{{ aTarget.sImageType }}"
                 alt="{{ aTarget.sToolTip }}"
            />
        </span>
    </a>
{% endfor %}
</div>
